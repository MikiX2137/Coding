#include <iostream>
#include <cstdlib>
#include <ctime>



int main(int argc, char* argv[])
{
    int tab[50];
    std::cout << "Losowanie liczb lotto";
    for (int i = 0; i < 6; i++)
    {
        tab[i] = 1 + rand() % (49 - 1 + 1);
        std::cout << tab[i] << ","<<std::endl;
    }
    system("PAUSE");
    return 0;
}
